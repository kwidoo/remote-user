{
    "name": "kwidoo/remote-user",
    "description": "Laravel remote user provider",
    "keywords": [
        "kwidoo",
        "laravel-remote-user"
    ],
    "homepage": "https://github.com/kwidoo/remote-user",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Oleg Pashkovsky",
            "email": "oleg@pashkovsky.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^11.0",
        "calebporzio/sushi": "^2.5"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "Kwidoo\\RemoteUser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kwidoo\\RemoteUser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kwidoo\\RemoteUser\\RemoteUserServiceProvider"
            ]
        }
    }
}
